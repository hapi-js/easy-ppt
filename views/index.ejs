<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>ppt目录</title>
  <link rel="stylesheet" href="/css/index.css">
</head>

<body>
  <ul>
    <% dir.forEach(function(d){ %>
    <li>
      <a href="/index/<%=d.fileName%> ">
        <img src="<%=d.cover%>" alt="<%=d.fileName%>" title="<%=d.fileName%>">
        <span><%=d.fileName%></span>
      </a>
    </li>
    <% }); %>
  </ul>
</body>

</html>